{
  "name": "Focus Tab",
  "description": "A distraction free new tab experience.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [],
  "action": {
    "default_popup": "index.html",
    "default_icon":{
      "16": "/assets/images/focus_16.png",
      "32": "/assets/images/focus_32.png",
      "64": "/assets/images/focus_64.png",
      "128": "/assets/images/focus_128.png"
    }
  },
  "icons": {
    "16": "/assets/images/focus_16.png",
    "32": "/assets/images/focus_32.png",
    "64": "/assets/images/focus_64.png",
    "128": "/assets/images/focus_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "/content/index.html"
  }
}