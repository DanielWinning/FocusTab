{
  "name": "Focus Tab",
  "description": "A distraction free new tab experience.",
  "version": "1.0.2",
  "manifest_version": 3,
  "permissions": [],
  "action": {
    "default_popup": "index.html",
    "default_icon":{
      "16": "icon16.png",
      "32": "icon32.png",
      "64": "icon64.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "/icon16.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "/content/index.html"
  }
}